/**app.wxss**/
@import "./styles/iconfont.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* padding 内边距  margin 外边距 */
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;   
  margin: 0;
  box-sizing: border-box;
}
page{
  --themeColor:#eb4450;
  font-size: 28rpx;
}
